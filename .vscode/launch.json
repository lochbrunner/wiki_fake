            "console": "integratedTerminal"
        },
        {
            "name": "Gan",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/gan.py",
            "args": [
                "-i",
                "data/processed.2.pickle",
                "-m",
                "data/snapshot.3.pickle"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        }
    ]
